<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">成绩信息详情</view>
  <view class="scoreInfo_detail_view"> 
      <view class="viewItem">
        <view class="label">记录编号：</view>
        <view class="content">
          <view>{{scoreId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">学生：</view>
        <view class="content"> 
          <view>{{studentObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">课程：</view>
        <view class="content"> 
          <view>{{courseObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">成绩得分：</view>
        <view class="content"> 
          <view>{{scoreValue}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">学生评价：</view>
        <view class="content"> 
          <view>{{studentEvaluate}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
